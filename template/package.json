{
  "name": "nextjs-simple-blog-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:indexing": "pagefind --site out --output-path public/pagefind",
    "build": "next build",
    "postbuild": "pagefind --site out --output-path out/pagefind && next-sitemap",
    "preview": "http-server out -p 3000",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "fmt": "prettier --ignore-path .gitignore --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "fmt:fix": "prettier --write --ignore-path .gitignore --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "typecheck": "tsc --noEmit",
    "test": "exit 0"
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "@next/third-parties": "16.1.6",
    "cheerio": "1.2.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "encoding": "0.1.13",
    "highlight.js": "11.11.1",
    "lucide-react": "0.563.0",
    "lucide-static": "0.563.0",
    "microcms-js-sdk": "3.3.0",
    "next": "16.1.6",
    "qs": "6.14.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-share": "5.2.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "25.2.0",
    "@types/qs": "6.14.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-wandfuldays": "workspace:*",
    "http-server": "14.1.1",
    "next-sitemap": "4.2.3",
    "pagefind": "1.4.0",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  }
}
