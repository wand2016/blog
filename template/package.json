{
  "name": "nextjs-simple-blog-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:indexing": "pagefind --site out --output-path public/pagefind",
    "build": "next build",
    "postbuild": "pagefind --site out --output-path out/pagefind && next-sitemap",
    "preview": "http-server out -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "fmt": "prettier --ignore-path .gitignore --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "fmt:fix": "prettier --write --ignore-path .gitignore --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "typecheck": "tsc --noEmit",
    "test": "exit 0",
    "check-license": "license-checker --summary | grep -qvzi gpl"
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "@next/third-parties": "15.3.2",
    "cheerio": "1.0.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "encoding": "0.1.13",
    "highlight.js": "11.11.1",
    "lucide-react": "0.511.0",
    "lucide-static": "0.511.0",
    "microcms-js-sdk": "3.2.0",
    "next": "15.3.2",
    "qs": "6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-share": "5.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@tailwindcss/postcss": "4.1.7",
    "@types/node": "22.15.29",
    "@types/qs": "6.14.0",
    "@types/react": "19.1.5",
    "@types/react-dom": "19.1.5",
    "eslint": "9.27.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-wandfuldays": "workspace:*",
    "http-server": "14.1.1",
    "license-checker": "25.0.1",
    "next-sitemap": "4.2.3",
    "pagefind": "1.3.0",
    "postcss": "8.5.4",
    "prettier": "3.5.3",
    "tailwindcss": "4.1.7",
    "typescript": "5.8.3"
  }
}
